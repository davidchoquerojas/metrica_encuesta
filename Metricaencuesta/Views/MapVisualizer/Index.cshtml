@{
    Layout = null;
}
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml" ng-app="app">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="~/Content/bootstrap.min.css" />
    <link rel="stylesheet" href="~/Content/mapVisualizer.min.css" />
    <title>Map Visualizer</title>
</head>
<body ng-controller="mapCtrl">
    <div class="container-fluid" id="box-control">
        <div class="row box-header">
            <div class="col-md-12">
                <form class="form-inline">
                    <div class="form-group">
                        <label>Empleado</label>
                        <select class="form-control" ng-model="asistencia.id_empleado" ng-options="i.id_empleado as i.nombres +' '+ i.apellidos for i in listEmploye">
                            <option value=""> All </option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label>Fecha</label>
                        <input type="date" class="form-control" ng-model="asistencia.fecha_asistencia" />
                    </div>

                    <div class="form-group">
                        <label>Tipo Acción</label>
                        <select class="form-control" ng-model="asistencia.tipo_asistencia" ng-options="t.code as t.text for t in asisType" ng-change="viewMarker(asistencia)">
                            <option value=""> --- Seleccione --- </option>
                        </select>
                    </div>
                    <div class="form-group pull-right">
                        <a ng-if="toggle == 1" href="#" ng-click="toggleIn()" class="hidden-box" title="Ocultar"><span class="glyphicon glyphicon-resize-small"></span></a>
                        <a ng-if="toggle == 2" href="#" ng-click="toggleOn()" class="hidden-box" title="Abrir"><span class="glyphicon glyphicon-resize-full"></span></a>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <div id="map"></div>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBh_udrWqHlTgqAzmYEK1oc-69XyCNx44g"></script>

    <script src="~/Scripts/jquery-2.1.4.min.js"></script>
    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/Scripts/angular-locale_es-pe.js"></script>
    <script src="~/Scripts/angular-resource.min.js"></script>
    <script src="~/Scripts/appjs/app-factory.js"></script>
    <script src="~/Scripts/appjs/app-service.js"></script>

    <script src="~/Scripts/appjs/map_visualizer-controller.js"></script>
</body>
</html>